<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoCatalogo.Pages.MenuView"
			 BindingContext="{Binding Menu, Source={StaticResource Locator}}"
			 Title="Menu">
	<StackLayout>
		<Button Text="Catalogo" Command="{Binding NavigateCommand}" CommandParameter="List"></Button>
		<Button Text="Agregar Producto" Command="{Binding NavigateCommand}" CommandParameter="Edit"></Button>
	</StackLayout>
</ContentPage>